      <filename>nb-NO.com_virtuemart.ini</filename>
      <filename>nb-NO.com_virtuemart.sys.ini</filename>
      <filename>nb-NO.mod_virtuemart_cart.ini</filename>
      <filename>nb-NO.mod_virtuemart_category.ini</filename>
